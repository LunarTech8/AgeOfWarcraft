package Unit_ShepherdHut
import BuildingPreset


// --------------------
// Data code
// --------------------

@compiletime function genObj()
    new BuildingPreset(UnitIdsCustom.shepherdHut, UnitIds.farm)
    // Abilities:
    ..presetNormalAbilities()
    // Art:
    ..setGroundTexture("OSMA")  // Orc Shipyard, Orc Burrow, ...
    ..setIconGameInterface(Icons.bTNPolymorph)
    ..presetMaximumAngles(5., 5.)
    ..setModelFile(ImportPaths.multiHut)
    ..setRequiredAnimationNames("upgrade,second")
    ..setSelectionScale(3.)
    // Pathing:
    ..setCollisionSize(96.)
    ..setPathingMap("PathTextures\\6x6SimpleSolid.tga")
    // Sound:
    ..setUnitSoundSet("")
    // Stats:
    ..presetNoGoldAndLumber()
    ..setFoodProduced(0)
    ..presetSightRadius(0, 0)
    // Techtree:
    ..presetUpgradesUsed()
    // Text:
    ..presetNameAndTooltips(true, "Shepherd Hut", "")